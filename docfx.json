{
  "metadata": [
    {
      "src": [
        {
          "files": ["Plugin.ByteArrays/Plugin.ByteArrays.csproj"]
        }
      ],
      "includePrivateMembers": false,
      "disableGitFeatures": false,
      "disableDefaultFilter": false,
      "noRestore": true,
      "noBuild": true,
      "namespaceLayout": "flattened",
      "memberLayout": "samePage",
      "allowCompilationErrors": false,
      "apiFilter": "public"
    }
  ],
  "build": {
    "content": [{ "files": "**/*.{md,yml}" }, "toc.yml"],
    "globalMetadata": {
      "_appName": "Plugin.ByteArrays Documentation",
      "_appTitle": "Plugin.ByteArrays",
      "_enableSearch": true,
      "_appFooter": "By Francois Raminosona - Licensed under the MIT License",
      "_disableContribution": false,
      "pdf": false
    },
    "template": ["default", "modern"]
  },
  "serve": {
    "host": "localhost",
    "port": 8080
  }
}
